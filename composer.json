{
    "name": "linkorb/haigha",
    "description": "Alice fixtures for database tables",
    "keywords": ["alice", "wonderland", "fixtures", "march hare"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joost Faassen",
            "email": "j.faassen@linkorb.com",
            "homepage": "http://www.linkorb.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/linkorb/haigha/issues"
    },
    "require": {
        "php": "^7.1",
        "symfony/dotenv": "^3.4|^4.3|^5.0",
        "ramsey/uuid": "^2.0|^3.0",
        "nelmio/alice": "^3.0",
        "symfony/console": "^3.4|^4.3|^5.0",
        "linkorb/database-manager": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "bin": ["bin/haigha"],
    "autoload": {
        "psr-4": {
            "Haigha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haigha\\Tests\\": "tests/"
        }
    }
}
